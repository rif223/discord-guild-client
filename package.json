{
  "name": "discord-guild-client",
  "version": "0.2.2",
  "description": "This is a discord guild libery!",
  "type": "module",
  "types": "./typings/index.d.ts",
  "main": "./lib/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./typings/index.d.mts",
        "default": "./lib/index.js"
      },
      "require": {
        "types": "./typings/index.d.ts",
        "default": "./lib/index.js"
      }
    }
  },
  "scripts": {
    "test": "npm run build && node test/main.js",
    "build": "run-p -s build:*",
    "build:main": "babel -x .ts -d lib src",
    "build:types": "tsc",
    "docs": "typedoc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rif223/discord-guild-client.git"
  },
  "author": "rif223",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.7",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/preset-env": "^7.25.8",
    "@babel/preset-typescript": "^7.22.15",
    "@types/node": "^22.5.4",
    "@types/ws": "^8.5.12",
    "babel-plugin-add-import-extension": "^1.6.0",
    "npm-run-all": "^4.1.5",
    "typedoc": "^0.27.4",
    "typescript": "^5.2.2"
  },
  "files": [
    "lib",
    "typings"
  ]
}
